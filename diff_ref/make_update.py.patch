+def slow_checkout(svn_cmd, url, path):
+    """
+    Manual rate-limited checkout.
+    1. Checks out empty directory.
+    2. Lists top-level libraries.
+    3. Updates them one by one with a delay to avoid HTTP 429.
+    """
+    print(f"Starting SLOW checkout for {url}")
+    
+    # 1. Checkout empty folder (creates the structure)
+    if call(svn_cmd + ["checkout", "--depth", "empty", url, path], exit_on_error=False) != 0:
+        print("Initial empty checkout failed.")
+        sys.exit(1)
+
+    # 2. Get list of libraries (boost, python, etc.)
+    entries_raw = check_output(svn_cmd + ["list", url], exit_on_error=False)
+    if not entries_raw:
+        print("Could not list SVN entries.")
+        sys.exit(1)
+        
+    entries = [e.strip().strip('/') for e in entries_raw.splitlines() if e.strip()]
+
+    # 3. Update individually
+    for i, entry in enumerate(entries):
+        print(f"[{i+1}/{len(entries)}] Downloading {entry}...")
+        target = os.path.join(path, entry)
+        
+        # Update this specific item
+        if call(svn_cmd + ["update", "--set-depth", "infinity", target], exit_on_error=False) != 0:
+             # Retry once if failed
+             print(f"Failed to download {entry}, retrying in 5s...")
+             time.sleep(5)
+             call(svn_cmd + ["cleanup", path], exit_on_error=False)
+             call(svn_cmd + ["update", "--set-depth", "infinity", target])
+        
+        # THE FIX: Wait 2 seconds between libraries to satisfy the server
+        time.sleep(2)
+
+
